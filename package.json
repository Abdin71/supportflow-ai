{
  "name": "supportflow-ai",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "frontend/user-interface",
    "frontend/admin-dashboard",
    "backend",
    "shared"
  ],
  "scripts": {
    "dev:user": "npm run dev --workspace=frontend/user-interface",
    "dev:admin": "npm run dev --workspace=frontend/admin-dashboard",
    "dev:backend": "npm run serve --workspace=backend",
    "build:user": "npm run build --workspace=frontend/user-interface",
    "build:admin": "npm run build --workspace=frontend/admin-dashboard",
    "build:backend": "npm run build --workspace=backend",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "firebase": "^12.6.0"
  },
  "devDependencies": {
    "typescript": "^5"
  }
}
